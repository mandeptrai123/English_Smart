<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_round_rect"
    tools:context=".TOEIC.Menu_Toeic.Toeic_Full">
    <TextView
        android:layout_margin="10dp"
        android:textSize="16dp"
        android:textColor="#000000"
        android:text="Time : 30:00:00"
        android:background="@drawable/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <RelativeLayout
        android:layout_centerInParent="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/choose_part1"
            android:textSize="22dp"
            android:textColor="#ffffff"
            android:background="@drawable/button2"
            android:text="Photo"
            android:layout_width="320dp"
            android:layout_height="60dp" />
        <Button
            android:layout_marginTop="3dp"
            android:background="@drawable/button2"
            android:textColor="#ffffff"
            android:textSize="22dp"
            android:text="Question Response"
            android:id="@+id/choose_part2"
            android:layout_below="@id/choose_part1"
            android:layout_width="320dp"
            android:layout_height="60dp"/>
        <Button
            android:text="Short conversation"
            android:textSize="22dp"
            android:background="@drawable/button2"
            android:textColor="#ffffff"
            android:layout_marginTop="3dp"
            android:layout_below="@id/choose_part2"
            android:id="@+id/choose_part3"
            android:layout_width="320dp"
            android:layout_height="60dp" />
        <Button
            android:text="Short talk"
            android:textSize="22dp"
            android:background="@drawable/button2"
            android:textColor="#ffffff"
            android:layout_marginTop="3dp"
            android:layout_below="@id/choose_part3"
            android:id="@+id/choose_part4"
            android:layout_width="320dp"
            android:layout_height="60dp" />
        <Button
            android:text="Incomplete Sentence"
            android:textSize="22dp"
            android:background="@drawable/button2"
            android:textColor="#ffffff"
            android:layout_marginTop="3dp"
            android:layout_below="@id/choose_part4"
            android:id="@+id/choose_part5"
            android:layout_width="320dp"
            android:layout_height="60dp" />
        <Button
            android:text="Text completion"
            android:textSize="22dp"
            android:background="@drawable/button2"
            android:textColor="#ffffff"
            android:layout_marginTop="3dp"
            android:layout_below="@id/choose_part5"
            android:id="@+id/choose_part6"
            android:layout_width="320dp"
            android:layout_height="60dp"/>
        <Button
            android:text="Reading comprehen"
            android:textSize="22dp"
            android:background="@drawable/button2"
            android:textColor="#ffffff"
            android:layout_marginTop="3dp"
            android:layout_below="@id/choose_part6"
            android:id="@+id/choose_part7"
            android:layout_width="320dp"
            android:layout_height="60dp" />
    </RelativeLayout>
</RelativeLayout>